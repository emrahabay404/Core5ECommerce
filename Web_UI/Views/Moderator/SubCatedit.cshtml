@using Entities.Concrete;
@model SubCategory

@{
    ViewData["Title"] = "SubCatedit";
    Layout = "~/Views/Shared/_Moderator.cshtml";
}


<form method="post" action="/Moderator/SubCatedit/">
    <input hidden value="@Model.SubCatID" class="form-control" name="SubCatID" />
    <input value="@Model.SubCatName" class="form-control" name="SubCatName" />
    <input value="@Model.CategoryID" class="form-control" name="CategoryID" />
    <input value="@Model.Status.ToString()" class="form-control" name="Status" />
    <button class="btn btn-success" type="submit">Güncelle</button>
</form>
